nano replica_set.yml
kubectl apply -f replica_set.yml
kubectl describe replicaset/frontend
kubectl delete replicaset frontend



apiVersion: apps/v1
kind: ReplicaSet
metadata:
  name: frontend
  labels:
    app: guestbook
    tier: frontend
spec:
  replicas: 2
  selector:
    matchLabels:
      tier: frontend
  template:
    metadata:
      labels:
        tier: frontend
    spec:
      containers:
      - name: webapp
        image: quay.io/gauravkumar9130/mywebapp
